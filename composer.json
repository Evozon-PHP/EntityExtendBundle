{
  "name": "evozon-php/entity-extend-bundle",
  "type": "symfony-bundle",
  "description": "Slight variation of the original pjarmalavicius/PjEntityExtendBundle - Easy doctrine entities extending",
  "keywords": [
    "doctrine extend mapping"
  ],
  "homepage": "http://evozon.com",
  "license": "proprietary",
  "authors": [
    {
      "name": "Paulius Jarmalavičius",
      "email": "paulius.jarmalavicius@gmail.com"
    },
    {
      "name": "Balazs Csaba",
      "homepage": "http://evozon.com",
      "email": "csaba.balazs@evozon.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/symfony": "^3.0 || ^4.0",
    "doctrine/orm": ">=2.2.3",
    "doctrine/doctrine-bundle": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Pj\\EntityExtendBundle\\": ""
    }
  }
}